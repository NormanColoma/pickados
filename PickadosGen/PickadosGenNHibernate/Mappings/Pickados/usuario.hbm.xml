<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="PickadosGenNHibernate"
		                   namespace="PickadosGenNHibernate.EN.Pickados">
		    
		    
			
				<class name="UsuarioEN" table="Usuario"> 
				  	
						<id name="Id" column="id" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Alias" type="string"> 
						<column name="alias"  unique="true" not-null="true"/>
					</property>
					<property name="Email" type="string"> 
						<column name="email"  unique="true" not-null="true"/>
					</property>
					<property name="Password" type="string"> 
						<column name="password"  not-null="true"/>
					</property>
					<property name="Created_at" type="timestamp"> 
						<column name="created_at"  not-null="true"/>
					</property>
					<property name="Updated_at" type="timestamp"> 
						<column name="updated_at"  not-null="true"/>
					</property>
					<property name="Nif" type="string"> 
						<column name="nif"  unique="true" not-null="true"/>
					</property>
					<property name="Admin" type="boolean"> 
						<column name="admin"  not-null="true"/>
					</property>
			
			
			
			
			
			
			
			
			
					
					
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="UsuarioENReadAllHQL">from UsuarioEN</query>
		  <query name="UsuarioENfindByUserHQL"><![CDATA[select u FROM UsuarioEN as u where alias = :name]]></query>
		</hibernate-mapping>
	